{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "array",
      "items": { "type": "string" }
    },
    "anchors": {
      "type": "array",
      "items": { "type": "string" }
    },
    "evolution": {
      "type": "array",
      "items": { "type": "string" }
    },
    "components": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "desc": { "type": "string" },
          "evolution": { "type": "string" },
          "visibility": { "type": "string" },
          "pos": { "type": "string" },
          "labelpos": { "type": "string" }
        },
        "required": ["name", "desc", "evolution", "visibility", "pos"]
      }
    },
    "links": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "src": { "type": "string" },
          "tgt": { "type": "string" }
        },
        "required": ["src", "tgt"]
      }
    },
    "evolve": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "desc": { "type": "string" },
          "pos": { "type": "string" },
          "labelpos": { "type": "string" }
        },
        "required": ["name"]
      }
    },
    "markets": {
      "type": "array",
      "items": { "type": "string" }
    },
    "pipelines": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "desc": { "type": "string" },
          "pos": { "type": "string" },
          "labelpos": { "type": "string" }
        },
        "required": ["name"]
      }
    },
    "pioneers": {
      "type": "array",
      "items": { "type": "string" }
    },
    "notes": {
      "type": "array",
      "items": { "type": "string" }
    },
    "blueline": {
      "type": "array",
      "items": { "type": "string" }
    },
    "style": {
      "type": "array",
      "items": { "type": "string" }
    },
    "annotations": {
      "type": "array",
      "items": { "type": "string" }
    },
    "comments": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "required": ["title", "components", "links", "evolve", "pipelines", "comments"]
}
